<dialogs>
  <dialogframe><frame type="out"><frame type="in">
    <frame type="out">
      <vbox>
        <frame type="etched_in" fill="true">
          <hbox>
            <image file="images/stock_yes_20.png" expand="true" fill="true"/>
            <label name="dialog_title" expand="true" fill="true"/>
            <image file="images/stock_yes_20.png" expand="true" fill="true"/>
          </hbox>
        </frame>
        <frame type="etched_in" expand="true" fill="true">
          <vbox name="dialog_area" margin="4" expand="true" fill="true"/>
        </frame>
        <frame name="buttonframe" type="etched_in" fill="true">
          <hbox margin="4" expand="true" fill="true">
            <label expand="true" fill="true"/>
            <button name="yes" visible="false">
              <hbox>
                <image file="images/stock_yes_20.png" fill="true"/>
                <label text="((_Yes))" use_underline="true" expand="true" fill="true"/>
              </hbox>
            </button>
            <label name="yes_space" expand="true" fill="true" visible="false"/>
            <button name="ok" visible="false">
              <hbox>
                <image file="images/stock_ok_20.png" fill="true"/>
                <label text="((_OK))" use_underline="true" expand="true" fill="true"/>
              </hbox>
            </button>
            <label name="ok_space" expand="true" fill="true" visible="false"/>
            <button name="apply" visible="false">
              <hbox>
                <image file="images/stock_apply_20.png" fill="true"/>
                <label text="((_Apply))" use_underline="true" expand="true" fill="true"/>
              </hbox>
            </button>
            <label name="apply_space" expand="true" fill="true" visible="false"/>
            <button name="cancel" visible="false">
              <hbox>
                <image file="images/stock_cancel_20.png" fill="true"/>
                <label text="((_Cancel))" use_underline="true" expand="true" fill="true"/>
              </hbox>
            </button>
            <label name="cancel_space" expand="true" fill="true" visible="false"/>
            <button name="no" visible="false">
              <hbox>
                <image file="images/stock_no_20.png" fill="true"/>
                <label text="((_No))" use_underline="true" expand="true" fill="true"/>
              </hbox>
            </button>
            <label name="no_space" expand="true" fill="true" visible="false"/>
            <button name="ignore" visible="false">
              <hbox>
                <image file="images/stock_cancel_20.png" fill="true"/>
                <label text="((_Ignore))" use_underline="true" expand="true" fill="true"/>
              </hbox>
            </button>
            <label name="ignore_space" expand="true" fill="true" visible="false"/>
          </hbox>
        </frame>
      </vbox>
    </frame>
  </frame></frame></dialogframe>
  <messagedialog>
    <hbox expand="true" fill="true" spacing="8">
      <image hook="ICON" fill="true"/>
      <label hook="LABEL" line_wrap="true" expand="true" fill="true"/>
    </hbox>
  </messagedialog>
  <helpdialog>
    <text id="msg" expand="true" fill="true"/>
  </helpdialog>
</dialogs>
